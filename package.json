{
  "name": "@tasleem96/ngx-accordion",
  "version": "16.0.1",
  "description": "Simple accordion control for your angular applications using bootstrap.",
  "license": "MIT",
  "readmeFilename": "README.md",
  "private": false,
  "author": {
    "name": "Umed Khudoiberdiev",
    "email": "pleerock.me@gmail.com"
  },
  "contributors": [
    {
      "name": "CloudIDEaaS Inc",
      "email": "support@cloudideaas.com",
      "url": "http://www.cloudideaas.com/hydra/index.htm"
    }
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/esm2022/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "esm2022": "./dist/esm2022/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "prestart": "npm run build",
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server -c bs-config.json",
    "build": "tsc",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "prepublishOnly": "npm run build",
    "build_lib": "ng build ngx-accordion",
    "copy-license": "cp ./LICENSE ./dist/ngx-accordion",
    "copy-readme": "cp ./README.md ./dist/ngx-accordion",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "npm_pack": "npm run copy-files && cd dist/ngx-accordion && npm pack",
    "package": "npm run build_lib && npm run npm_pack"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tasleem96/ngx-accordion.git"
  },
  "bugs": {
    "url": "https://github.com/tasleem96/ngx-accordion/issues"
  },
  "tags": [
    "accordion",
    "angular16",
    "typescript",
    "angular16-accordion"
  ],
  "peerDependencies": {
    "@angular/core": "^16.2.12"
  },
  "devDependencies": {
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/compiler-cli": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/forms": "^16.2.12",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@types/node": "^20.14.9",
    "bootstrap": "^5.3.3",
    "chai": "^3.4.1",
    "del": "^2.2.2",
    "es6-shim": "^0.35.2",
    "gulp": "^3.9.0",
    "gulp-file": "^0.3.0",
    "gulp-mocha": "^3.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-shell": "^0.5.1",
    "gulp-tslint": "^7.0.1",
    "gulp-typescript": "^3.1.4",
    "gulp-uglify": "^2.0.0",
    "gulpclass": "^0.1.1",
    "mocha": "^3.2.0",
    "ng-packagr": "^16.0.0",
    "reflect-metadata": "0.1.9",
    "rxjs": "^7.8.1",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "systemjs": "0.19.42",
    "tslib": "^2.3.0",
    "tslint": "^4.3.1",
    "tslint-stylish": "^2.1.0-beta",
    "typescript": "^4.9.5",
    "zone.js": "^0.13.3"
  }
}
