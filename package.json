{
  "name": "@tasleem96/ngx-accordion",
  "version": "16.0.6",
  "description": "Simple accordion control for your Angular applications using Bootstrap.",
  "license": "MIT",
  "readmeFilename": "README.md",
  "private": false,
  "author": {
    "name": "Umed Khudoiberdiev",
    "email": "pleerock.me@gmail.com"
  },
  "contributors": [
    {
      "name": "CloudIDEaaS Inc",
      "email": "support@cloudideaas.com",
      "url": "http://www.cloudideaas.com/hydra/index.htm"
    }
  ],
  "main": "dist/esm2022/index.mjs",
  "types": "dist/index.d.ts",
  "module": "dist/esm2022/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "esm2022": "./dist/esm2022/index.mjs",
      "default": "./dist/fesm2022/index.mjs"
    },
    "./package.json": {
      "default": "./package.json"
    }
  },
  "scripts": {
    "prestart": "npm run build",
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\"",
    "lite": "lite-server -c bs-config.json",
    "build": "ng-packagr -p ng-package.json",
    "prepublishOnly": "npm run build",
    "copy-readme": "cp ./README.md ./dist",
    "npm_pack": "npm run copy-readme && cd dist && npm pack",
    "package": "npm run build && npm run npm_pack"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tasleem96/ngx-accordion.git"
  },
  "bugs": {
    "url": "https://github.com/tasleem96/ngx-accordion/issues"
  },
  "tags": [
    "accordion",
    "angular16",
    "typescript",
    "angular16-accordion"
  ],
  "peerDependencies": {
    "@angular/core": "^16.2.12"
  },
  "devDependencies": {
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/compiler-cli": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/forms": "^16.2.12",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@types/node": "^20.14.9",
    "bootstrap": "^5.3.3",
    "concurrently": "^8.2.2",
    "lite-server": "^2.6.0",
    "ng-packagr": "^16.2.3",
    "rxjs": "^7.8.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "es6-shim": "^0.35.8",
    "reflect-metadata": "^0.2.2",
    "systemjs": "^6.15.1",
    "zone.js": "^0.13.3"
  }
}
